{
  "name": "vfg",
  "version": "0.0.2",
  "description": "vue3 form generator (element-plus).",
  "private": false,
  "main": "dist/lib.umd.js",
  "module": "dist/lib.es.js",
  "style": "dist/style.css",
  "exports": {
    ".": {
      "import": "dist/lib.es.js",
      "require": "dist/lib.umd.js"
    },
    "./*": "./*"
  },
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zhonghuitech/form-generator.git"
  },
  "keywords": [
    "vue",
    "form",
    "vue3",
    "generator",
    "element-plus"
  ],
  "author": "Aborn Jiang",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zhonghuitech/form-generator/issues"
  },
  "homepage": "https://github.com/zhonghuitech/form-generator#readme",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "test:unit": "vitest",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.3.1",
    "axios": "^1.7.7",
    "element-plus": "^2.8.4",
    "element-theme-chalk": "^2.15.14",
    "file-saver": "^2.0.5",
    "highlight.js": "^11.10.0",
    "js-beautify": "^1.15.1",
    "jsplumb": "^2.15.6",
    "sass": "^1.79.4",
    "vue": "^3.5.11",
    "vue-clipboard3": "^2.0.0",
    "vue-draggable-next": "^2.2.1",
    "vue-router": "^4.4.5"
  },
  "devDependencies": {
    "@tsconfig/node20": "^20.1.4",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^20.14.5",
    "@vitejs/plugin-vue": "^5.1.4",
    "@vitejs/plugin-vue-jsx": "^4.0.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.5.1",
    "fast-glob": "^3.3.2",
    "jsdom": "^24.1.0",
    "less": "^4.2.0",
    "npm-run-all2": "^6.2.0",
    "typescript": "~5.4.0",
    "vite": "^5.4.8",
    "vite-plugin-svg-icons": "2.0.1",
    "vite-plugin-vue-devtools": "^7.3.1",
    "vitest": "^1.6.0",
    "vue-tsc": "^2.0.21"
  }
}